// expandMessageString()
__ASSERT( expandMessageStrings("") === "" )
__ASSERT( expandMessageStrings("{}") === "{}" )
__ASSERT( expandMessageStrings("hoge") === "hoge" )
__ASSERT( expandMessageStrings("{fuga}") === "{fuga}" )
__ASSERT( expandMessageStrings("{piyo}", "piyo", "") === "" )
__ASSERT( expandMessageStrings("{piyo}", "piyo", "str") === "str" )
__ASSERT( expandMessageStrings("{foo} foo {bar} {bar}{bar}", "foo", "f", "bar", "b") === "f foo b bb" )
__ASSERT( expandMessageStrings("{foo} foo {bar} {1}{@@}", "foo", "f", "bar", "b", "1", 123, "@@" , "++") === "f foo b 123++" )
__ASSERT( expandMessageStrings("{}{TES}{{TEST}}{{TES}}{", "TES", "hoge") === "{}hoge{{TEST}}{hoge}{" )
__ASSERT( expandMessageStrings("{HOGE}{hoge}{fuga}{piyo}", "foo", "", "bar", "bar") === "{HOGE}{hoge}{fuga}{piyo}" )
